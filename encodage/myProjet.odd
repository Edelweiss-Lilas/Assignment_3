            <!--Checking module textstructure-->
            <moduleRef key="textstructure" include="TEI text body div listBibl bibl"/>
            <elementSpec ident="material" mode="change" module="msdescription">
               <desc>[Note Change!] I add material in my bibl element in order to clarify what I want to tell by that and not using edition.</desc>
               <classes mode="change">
                  <memberOf key="model.phrase" mode="add"/>
               </classes>
               <attList>
                  <attDef ident="type" mode="change">
                     <desc>[Note Change!]In our project we use material to describe the book and limit the material of the book we could found in Marie-Antoinette's catalog.</desc>
                     <valList type="closed" mode="replace">
                        <valItem ident="velin"/>
                        <valItem ident="chine"/>
                        <valItem ident="hollande"/>
                        <valItem ident="maroquin"/>
                     </valList>
                  </attDef>
               </attList>
               <exemplum>
                  <egXML xmlns="http://www.tei-c.org/ns/Examples">
                     <bibl>
                        <title>Histoire du traité de Westphalie</title>;
                        <author>P.Bougeant</author>
                        <material>mar.r, fil, dos orné, tr.dor.</material> 
                        <placeName>Paris</placeName>,
                        <date>1751</date>                     
                        <biblScope unit="volume">6 vol.</biblScope> <biblScope unit="format">in-12</biblScope>                   
                        <affiliation>Bibliothèque nationale, Réserve</affiliation>
                        <idno>E.VII</idno>
                     </bibl>
                  </egXML>
               </exemplum>
            </elementSpec>
            <!-- Checking the URL in the teiHeader-->
            <elementSpec ident="ref" mode="change">
               <constraintSpec ident="check_url_protocol" mode="change" scheme="schematron">
                  <desc>[Note Change!]to guarantee the validity of the URL and the URL attribute has to start by "http" or "https" only.</desc>
                  <constraint>
                    <sch:rule context="tei:ref[@target]">
                       <sch:assert test="starts-with(@target,'http')">
                       </sch:assert>
                    </sch:rule>
                  </constraint>
               </constraintSpec>
